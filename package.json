{
  "name": "markwright",
  "version": "0.8.0",
  "description": "Markdown typesetting component for React.",
  "main": "dist/markwright.js",
  "types": "types/index.d.ts",
  "author": "Noah Lange <noahrlange@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "esbuild src/index.ts --external:react --external:simple-markdown --minify --bundle --outfile=\"dist/markwright.js\"",
    "docs": "vite build",
    "dev:lint": "eslint --fix './src/**/*.{ts,tsx}'",
    "dev:pretty": "prettier --write 'src/**/*'",
    "dev:types": "tsc --emitDeclarationOnly",
    "dev:test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-react": "^2.0.0",
    "esbuild": "^0.14.54",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "jsdom": "^20.0.0",
    "sass": "^1.54.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "simple-markdown": "^0.7.3"
  }
}
