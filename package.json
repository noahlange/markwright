{
  "name": "markwright",
  "version": "0.7.0",
  "description": "Markdown typesetting component for React.",
  "main": "dist/markwright.js",
  "author": "Noah Lange <noahrlange@gmail.com>",
  "license": "MIT",
  "scripts": {
    "pretty": "prettier --write src/**/*",
    "lint": "tslint --project .",
    "dev": "webpack-dev-server --config webpack.docs.js",
    "watch:mw": "webpack --watch",
    "build:mw": "NODE_ENV=production webpack",
    "build:docs": "NODE_ENV=production webpack --config webpack.docs.js"
  },
  "devDependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.4",
    "prettier": "^1.14.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-promise": "^2.0.3",
    "react-virtualized": "^9.20.1",
    "simple-markdown": "^0.4.1",
    "ts-loader": "^4.2.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.8.0",
    "typescript": "^2.7.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-promise": "^2.0.3",
    "react-virtualized": "^9.20.1",
    "simple-markdown": "^0.4.1"
  },
  "prettier": {
    "singleQuote": true
  }
}
